//工具类-util
namespace yh.util{

	换行符='\r\n';

	获取启动参数=function(ARGV,名称){
		if(!ARGV || #ARGV<1) return null; 
		var 名称=..string.lower(名称);
		for(i=1;#ARGV;1){
			var key=..string.lower(ARGV[i]);
			if( 名称 == key ){
				var k=i+1;
				if(k>#ARGV){ return null;}
				else { return ARGV[k]; }
			}
		}
		return null;
	}

}

